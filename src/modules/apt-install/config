[ -n "$APT-INSTALL_DISTUPGRADE" ] || APT-INSTALL_DISTUPGRADE=y # y/n
# upgrade vs full upgrade. 
[ -n "$APT-INSTALL_DISTUPGRADE_METHOD" ] || APT-INSTALL_DISTUPGRADE_METHOD=full-upgrade
[ -n "$APT-INSTALL_USE_PREINSTALLER" ] || APT-INSTALL_USE_PREINSTALLER=n # y/n
# Add Packages that should be APT-INSTALLed.
[ -n "$APT-INSTALL_PRE_INSTALL_PKGS" ] || APT-INSTALL_PRE_INSTALL_PKGS=""
