[ -n "$PREINSTALL_DISTUPGRADE" ] || PREINSTALL_DISTUPGRADE=y # y/n
# upgrade vs full upgrade. 
[ -n "$PREINSTALL_DISTUPGRADE_METHOD" ] || PREINSTALL_DISTUPGRADE_METHOD=full-upgrade
[ -n "$PREINSTALL_USE_PREINSTALLER" ] || PREINSTALL_USE_PREINSTALLER=n # y/n
# Add Packages that should be preinstalled.
[ -n "$PREINSTALL_PRE_INSTALL_PKGS" ] || PREINSTALL_PRE_INSTALL_PKGS=""
